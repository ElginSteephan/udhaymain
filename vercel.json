        { "type": "host", "value": "udhayship.com" }
      ],
      "destination": "https://www.udhayshipping.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.udhayship.com" }
      ],
      "destination": "https://www.udhayshipping.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/(.*)", "destination": "/" }
  ]
